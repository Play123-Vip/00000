local keyPromptTitle = gg.alert("KEY | SCRIPT | XT MODE PRO")
function insertKey()
  local key = "https://sfl.gl/NZUtgFp"
  gg.toast("KEY | SCRIPT | XT MODE PRO")
  gg.copyText(key)
end

menu = gg.choice({"REGISTER", "LOGIN"}, nil, "XT MODE PRO")
if menu == 1 then
  insertKey()
end

-- Dua key yang valid
local key1 = "XTKeyDEV-0000"
local key2 = "XTKey8702674"

-- Minta user input satu key saja
local input = gg.prompt({"Masukkan Key:"}, nil, {"text"})

if input == nil then
  gg.alert("❌ Tidak ada input. Keluar.")
  os.exit()
elseif input[1] == key1 or input[1] == key2 then
  gg.alert("✅ Key benar! Script aktif.")
else
  gg.alert("❌ Key salah! Script keluar.")
  os.exit()
end

gg.alert("🔥XT MODE PRO 🔥\n🎄☃️", "⚡XT MODE PRO MENU⚡")

function script()
  menuprincipal = gg.multiChoice({
    "HEADSHOT SAFE",
    "❌Exit🔙"
  }, nil, "🎅🏻Script Vip Premium🎅🏻\n🌠TELEGRAM: XT MODE PRO 🌠\n☃️Youtube: XT MODE PRO ™☃️\n")

  if menuprincipal == nil then
    gg.toast("☃️Subscríbete XT MODE PRO ☃️")
  else
    if menuprincipal[1] then vip() end
    if menuprincipal[2] then
      gg.setVisible(true)
      os.exit(print("☃️ Subscribe XT MODE PRO "))
    end
  end
end

function vip()
gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber(';bone_Spine')
gg.getResults(gg.getResultsCount())
gg.editAll(';bone_Neck', gg.TYPE_WORD)
gg.clearResults()
gg.searchNumber('-0.04462028295', gg.TYPE_FLOAT)
gg.getResults(gg.getResultsCount())
gg.editAll('-0.03999999911', gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber('-0.03881515563', gg.TYPE_FLOAT)
gg.getResults(gg.getResultsCount())
gg.editAll('0.05000000075', gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber('0.07869631797;0.99689865112;1;1;1::17', gg.TYPE_FLOAT)
gg.refineNumber('1', gg.TYPE_FLOAT)
gg.getResults(gg.getResultsCount())
gg.editAll('-0.1', gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber('0.98958933353F;1;1;1::17', gg.TYPE_FLOAT)
gg.refineNumber('1', gg.TYPE_FLOAT)
gg.getResults(gg.getResultsCount())
gg.editAll('-1.5', gg.TYPE_FLOAT)
gg.clearResults()
gg.toast('HEADSHOT SAFE AKTIF')
end

while true do if gg.isVisible() then gg.setVisible(false) script() end end