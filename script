function vip()
  gg.editAll("67109633", gg.TYPE_DWORD)
gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_CODE_APP)
gg.searchNumber("h 5A 55 A5 E3 B5 51 CD E1")
gg.getResults(155)
gg.editAll("h 55 F5 25 E3", gg.TYPE_BYTE)
gg.clearResults()  
gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("h 69 00 6E 00 67 00 61 00 6D 00 65 00", gg.TYPE_BYTE)
  gg.getResults(500)
  gg.editAll("h650066006600650063007400", gg.TYPE_BYTE)
  gg.clearResults()
gg.toast('ğ‘©ğ’€ğ‘·ğ‘¨ğ‘ºğ‘º ğ‘½ğ‘°ğ‘· ğ‘¨ğ‘²ğ‘»ğ‘°ğ‘­')
end

function Antenna()
  gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber('-0.02980032004;1:5', gg.TYPE_FLOAT)
gg.getResults(16)
