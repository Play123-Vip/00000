  gg.alert("‚ùå Tidak ada input. Keluar.")
  os.exit()
elseif input[1] == key1 or input[1] == key2 then
  gg.alert("‚úÖ Key benar! Script aktif.")
else
  gg.alert("‚ùå Key salah! Script keluar.")
  os.exit()
end


