local keyPromptTitle = gg.alert("KEY | SCRIPT | XT MODE PRO")
function insertKey()
  local key = "4"
  gg.toast("KEY | SCRIPT | XT MODE PRO")
  gg.copyText(key)
end

menu = gg.choice({"COPY LINK | KEY NEW", "LOGIN"}, nil, "Select an option:")
if menu == 1 then
  insertKey()
end

--[[ XT MODE PRO KEY otomatis harian ]]
-- TELEGRAM @XTMODEPRO

local today = os.date("%d%m%Y") -- misalnya: 26072025
local salt = 987654
local number = tonumber(today) + salt
local key = "XTKey" .. tostring(number):sub(-6)

local input = gg.prompt({"🛡️ Masukkan KEY hari ini:"}, {""})[1]

if input == key then
    gg.alert("✅ KEY BENAR! XT MODE PRO aktif...")
    -- taruh script fitur kamu di bawah ini
    gg.toast("🔥 Selamat datang di XT MODE PRO!")
else
    gg.alert("❌ KEY SALAH! Script keluar.")
    os.exit()
end

gg.alert("🔥XT MODE PRO 🔥\n🎄☃️", "⚡XT MODE PRO MENU⚡")

function script()
  menuprincipal = gg.multiChoice({
    "𝑩𝒀𝑷𝑨𝑺𝑺 𝑽𝑰𝑷",
    "𝑨𝑵𝑻𝑬𝑵𝑨 ✋",
    "𝑨𝑵𝑻𝑬𝑵𝑨 𝑯𝑬𝑨𝑫 🗼",
    "𝑪𝑨𝑴𝑬𝑹𝑨 𝑯𝑨𝑪𝑲",
    "𝑯𝑬𝑨𝑫𝑺𝑯𝑶𝑻 𝑩𝑶𝑫𝒀",
    "𝑴𝑨𝑮𝑰𝑪 𝑩𝑼𝑳𝑳𝑬𝑻",
    "𝑺𝑷𝑬𝑬𝑫 𝑹𝑼𝑵 𝟑𝑿",
    "𝑰𝑵𝑽𝑰𝑺𝑰𝑩𝑳𝑬 𝑮𝑳𝑶𝑾𝑨𝑳𝑳",
    "❌Exit🔙"
  }, nil, "🎅🏻Script Vip Premium🎅🏻\n🌠TELEGRAM: XT MODE PRO 🌠\n☃️Youtube: XT MODE PRO ™☃️\n")

  if menuprincipal == nil then
    gg.toast("☃️Subscríbete XT MODE PRO ☃️")
  else
    if menuprincipal[1] then vip() end
    if menuprincipal[2] then Antenna() end
    if menuprincipal[3] then Antenna_Head() end
    if menuprincipal[4] then CAMERA() end
    if menuprincipal[5] then HEADSHOT() end
    if menuprincipal[6] then Magic() end
    if menuprincipal[7] then SP() end
    if menuprincipal[8] then Invisible_Glowall() end
    if menuprincipal[9] then
      gg.setVisible(true)
      os.exit(print("☃️ Subscribe XT MODE PRO "))
    end
  end
end

