function insertKey()
  local key = "https://sfl.gl/NZUtgFp"
  gg.toast("KEY | SCRIPT | XT MODE PRO")
  gg.copyText(key)
end

menu = gg.choice({"REGISTER", "LOGIN"}, nil, "XT MODE PRO")
if menu == 1 then
  insertKey()
end

