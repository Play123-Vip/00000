  gg.searchNumber(";Left_Weapon", gg.TYPE_WORD, nil, nil, nil, nil)
  gg.getResults(1045)
  gg.editAll(";Spine", gg.TYPE_WORD)
  gg.clearResults()
  gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("-1197037021;1892485702", gg.TYPE_DWORD)
  gg.getResults(221)
  gg.refineNumber("1892485702", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(109)
  gg.setVisible(false)
